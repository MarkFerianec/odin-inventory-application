<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Items</title>
  </head>
  <body>
    <%- include('navbar', {links: links}) %>
    <h1>Items</h1>
    <ul>
      <% items.forEach((item) => { %>
      <li>
        Brand: <%= item.brand %> Gender: <%= item.gender %> Type: <%= item.type
        %> Quantity: <%= item.quantity %> Price: $<%= item.price %>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
