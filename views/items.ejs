<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Items</title>
  </head>
  <body>
    <%- include('navbar', {links: links}) %>
    <h1>Items</h1>
    <% items.forEach((item) => { %>
    <l1>
      Brand: <%= item.brand %> Gender: <%= item.gender %> Type: <%= item.type %> Quantity: <%= item.quantity %>
      Price: <%= item.price %>
    </l1>
    <% }) %>
  </body>
</html>
